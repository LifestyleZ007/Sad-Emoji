<script type="text/javascript">
	var c = document.getElementById("myCanvas");
	var ctx = c.getContext("2d");

	
	ctx.beginPath();
	ctx.arc(200,220,90,.996,.680*Math.PI);
	ctx.stroke();

	ctx.beginPath();
	ctx.arc(200,200,150,0,2*Math.PI);
	ctx.stroke();

	ctx.beginPath();
	ctx.arc(120,110,60,.96,.70*Math.PI);
	ctx.stroke();

	ctx.beginPath();
	ctx.arc(270,110,60,.96,.70*Math.PI);
	ctx.stroke();

	ctx.beginPath();
	ctx.arc(200,200,50,1.2,0.60*Math.PI);
	ctx.stroke();

td = canvas(root)
dim = 30,50,30,50
sk = td.create_arc(root, dim, bg='white')



